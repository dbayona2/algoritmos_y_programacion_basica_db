Algoritmo Ejercicio_17_presupuesto_hospital
	//entradas
	Escribir "Presupuesto asignado"
	Leer presupuestoa
	//caja negra
	Ginecologia=(0.4 * presupuestoa)
	Traumatologia= (0.3 * presupuestoa)
	Pediatria= (0.3 * presupuestoa)
