Algoritmo Ejercicio_5
	Escribir ("Digite el número 1: ")
	Leer numero1
	Escribir ("Digite el número 2: ")
	Leer numero2
	Si numero1>numero2 Entonces
		Escribir ("Mayor, numero1")
	Sino Si numero1<numero2 Entonces
			Escribir ("Mayor, numero2")
		SiNo
			Escribir ("Ambos son iguales")
		FinSi
	Fin Si
FinAlgoritmo
